<persistence xmlns="http://java.sun.com/xml/ns/persistence"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
		version="1.0">

	<persistence-unit name="statisticsPer" transaction-type="RESOURCE_LOCAL">
	<provider>org.hibernate.ejb.HibernatePersistence</provider>
	<jar-file></jar-file>
	<class>org.mxhero.engine.plugin.statistics.internal.entity.Stat</class> 
	<class>org.mxhero.engine.plugin.statistics.internal.entity.Record</class>
	<class>org.mxhero.engine.plugin.statistics.internal.entity.StatPk</class> 
	<class>org.mxhero.engine.plugin.statistics.internal.entity.RecordPk</class>	
	<exclude-unlisted-classes/>
	</persistence-unit>

</persistence>