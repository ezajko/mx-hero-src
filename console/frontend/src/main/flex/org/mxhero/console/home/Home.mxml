		   xmlns:mx="library://ns.adobe.com/flex/mx" 		   
		   layout="vertical" 
		   width="100%" 
		   height="100%" 
		   xmlns:parsley="http://www.spicefactory.org/parsley" 
		   xmlns:presentation="org.mxhero.console.home.presentation.*">
	<fx:Script>
		<![CDATA[
			import org.mxhero.console.home.presentation.HomeView;
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<parsley:ContextBuilder>
			<parsley:FlexConfig type="{HomeContext}"/>
		</parsley:ContextBuilder>
		<parsley:Configure/>
	</fx:Declarations>
	
	<fx:Style source="../../../../../resources/styles/mxhero.css"/>
	
	<presentation:HomeView id="container" width="100%" height="100%"/>
	
</mx:Module>
