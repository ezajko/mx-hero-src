<?xml version="1.0" encoding="utf-8"?>
<mx:Module xmlns:fx="http://ns.adobe.com/mxml/2009" 
		   xmlns:s="library://ns.adobe.com/flex/spark" 
		   xmlns:mx="library://ns.adobe.com/flex/mx" 
		   xmlns:parsley="http://www.spicefactory.org/parsley" 
		   xmlns:presentation="org.mxhero.console.policies.presentation.*"
		   layout="vertical" 
		   width="100%" 
		   height="100%">
	<fx:Declarations>
		<parsley:ContextBuilder>
			<parsley:FlexConfig type="{PoliciesContext}"/>
		</parsley:ContextBuilder>
		<parsley:Configure/>
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[
			import org.mxhero.console.frontend.application.MainDestination;
			import org.mxhero.console.policies.presentation.PoliciesViewPM;
	
			[Inject]
			[Bindable]
			public var model:PoliciesViewPM;
			
		]]>
	</fx:Script>
	
	<presentation:PoliciesView id="container" width="100%" height="100%"/>
</mx:Module>
