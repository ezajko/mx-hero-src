<?xml version="1.0" encoding="utf-8"?>
<s:Module xmlns:fx="http://ns.adobe.com/mxml/2009" 
		   xmlns:s="library://ns.adobe.com/flex/spark" 
		   xmlns:mx="library://ns.adobe.com/flex/mx" 
		   xmlns:parsley="http://www.spicefactory.org/parsley" 
		   xmlns:presentation="org.mxhero.console.reports.presentation.*"
		   width="100%" 
		   height="100%" 
		   creationComplete="{ReportsViewPM.container=container}">
	
	<fx:Declarations>
		<parsley:ContextBuilder>
			<parsley:FlexConfig type="{ReportsContext}"/>
		</parsley:ContextBuilder>
		<parsley:Configure/>
	</fx:Declarations>
	
	<fx:Style source="../../../../../resources/styles/mxhero.css"/>
	
	<fx:Script>
		<![CDATA[
			import org.mxhero.console.reports.presentation.ReportsViewPM;
			
		]]>
	</fx:Script>
	<s:layout>
		<s:VerticalLayout/>
	</s:layout>
	<presentation:ReportsView id="container" width="100%" height="100%"/>
</s:Module>
