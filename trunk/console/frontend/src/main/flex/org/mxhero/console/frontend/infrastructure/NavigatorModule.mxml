<?xml version="1.0" encoding="utf-8"?>
<s:NavigatorContent xmlns:fx="http://ns.adobe.com/mxml/2009" 
					xmlns:s="library://ns.adobe.com/flex/spark" 
					xmlns:mx="library://ns.adobe.com/flex/mx" 
					width="100%" 
					height="100%"
					label="{moduleData.name}">
	
	<fx:Declarations>
		
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[
			
			private var _moduleData:ModuleData;
			
			[Bindable]
			public function get moduleData():ModuleData
			{
				return _moduleData;
			}

			public function set moduleData(value:ModuleData):void
			{
				if(value!=null){
					value.moduleLoader=moduleLoader;
				}
				_moduleData = value;		
			}

		]]>
	</fx:Script>
	
	<mx:ModuleLoader id="moduleLoader" width="100%" height="100%"/>
	
</s:NavigatorContent>
